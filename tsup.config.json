{
    "entry": ["src/app.ts"],
    "dts": false,
    "format": ["cjs"],
    "target": "es5",
    "minify": "terser",
    "minifyWhitespace": true,
    "minifyIdentifiers": true,
    "minifySyntax": true,
    "treeshake": true,
    "cjsInterop": true,
    "removeNodeProtocol": true,
    "clean": true,
    "onSuccess": "pnpm upload"
}
