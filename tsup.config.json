{
    "name": "tsup",
    "entry": ["src/index.js"],
    "dts": false,
    "format": ["cjs"],
    "target": "es5",
    "minify": "terser",
    "minifyWhitespace": true,
    "minifyIdentifiers": true,
    "minifySyntax": true,
    "treeshake": true,
    "cjsInterop": true,
    "clean": true,
    "onSuccess": "pnpm upload",
    "noExternal": ["core-js"]
}
