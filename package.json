{
    "name": "muse-typescript",
    "description": "Playground project for experimenting with TypeScript and Muse",
    "version": "0.1.0",
    "license": "MIT",
    "keywords": [
        "typescript",
        "muse",
        "playground",
        "amx",
        "harman"
    ],
    "main": "dist/main.js",
    "scripts": {
        "prebuild": "pnpm lint",
        "build": "tsup",
        "lint": "tsc --noEmit",
        "ask": "run-script-os",
        "ask:win32": "pwsh ./scripts/ask.ps1",
        "ask:darwin:linux": "bash ./scripts/ask.sh",
        "upload": "run-script-os",
        "upload:win32": "pwsh ./scripts/upload.ps1",
        "upload:darwin:linux": "bash ./scripts/upload.sh"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.1",
        "@commitlint/config-conventional": "^19.6.0",
        "@swc/core": "^1.10.1",
        "@types/core-js": "^2.5.8",
        "@types/lodash": "^4.17.13",
        "@types/node": "^22.10.2",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.11",
        "run-script-os": "^1.1.6",
        "terser": "^5.37.0",
        "tsup": "^8.3.5",
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "core-js": "^3.39.0",
        "lodash": "^4.17.21",
        "redux": "^5.0.1"
    }
}
